{
    "name": "responder-bridge-monorepo",
    "lockfileVersion": 3,
    "requires": true,
    "packages": {
        "": {
            "name": "responder-bridge-monorepo",
            "workspaces": [
                "packages/*"
            ]
        },
        "node_modules/@hak2i/responder-bridge": {
            "resolved": "packages/responder-bridge",
            "link": true
        },
        "node_modules/@hak2i/responder-bridge-openai": {
            "resolved": "packages/responder-bridge-openai",
            "link": true
        },
        "packages/responder-bridge": {
            "name": "@hak2i/responder-bridge",
            "version": "0.1.0",
            "license": "MIT",
            "dependencies": {
                "@hak2i/responder-bridge-openai": "^0.1.0"
            },
            "bin": {
                "responder-bridge": "bin/responder-bridge.js"
            }
        },
        "packages/responder-bridge-openai": {
            "name": "@hak2i/responder-bridge-openai",
            "version": "0.1.0",
            "license": "MIT"
        }
    }
}
